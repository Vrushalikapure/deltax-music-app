
<!-- include header -->
    <%- include('include/_header') %>
<!-- /include header -->


    <!-- Main Site -->
    <main id="site-main">
        <div class="container">
            <div class="box-nav d-flex justify-between">
                <a href="/add-song" class="border-shadow">
                    <span class="text-gradient">New Song <i class="fa-solid fa-music"></i></span>
                </a>
            </div>

            <div class="box-nav d-flex justify-between" style="float: right";>
                <form action="/logout" method="POST">
                    <button class="text-gradient">Logout <span class="text-gradient"> <i class="fa-solid fa-arrow-right-from-bracket"></i></span></button>
                </form>
            </div>

            <div class="header">
                <h1>Top 10 Songs</h1>
              </div>
            <!-- form handling -->
            <form action="/" method="POST">
                <table class="table">
                    <thead class="thead-dark">
                        <tr>
                            <th>Artwork</th>
                            <th>Song</th>
                            <th>Date of Release</th>
                            <th>Artists</th>
                            <th>Rating</th>
                        </tr>
                    </thead>
                    <tbody>
                        <%- include('include/_top_10_songs') %>
                    </tbody>
                </table>
            </form>
        </div>
    </main>
<!-- /Main Site -->


